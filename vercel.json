{
  "builds": [
    { "src": "app.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/my_blueprint/(.*)", "dest": "app.py" },
    { "src": "(/__wasm-worker.js)", "dest": "app.py" },
    { "src": "(/static/.*)", "dest": "app.py" },
    { "src": "/(.*)", "dest": "app.py" }
  ]
}